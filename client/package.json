{
  "name": "client",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host --port 8070 --mode ${NODE_ENV:=development}",
    "build": "npm run type-check && npm run build-only",
    "preview": "vite preview",
    "build-only-orig": "vite build",
    "build-only": "vite build --mode ${NODE_ENV:=development} --config vite-${NODE_ENV:=development}.config.mts",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/**/*.ts src/**/*.vue src/*.vue src/*.ts "
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.5",
    "@mdi/font": "7.4.47",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vueuse/core": "^13.3.0",
    "axios": "^1.9.0",
    "mitt": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.3.0",
    "vue": "^3.5.15",
    "vuetify": "^3.8.7"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.29",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/eslint-plugin": "^1.2.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.27.0",
    "eslint-config-vuetify": "^3.0.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "npm-run-all2": "^7.0.2",
    "pinia": "^3.0.2",
    "sass-embedded": "^1.89.1",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.7.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-layouts-next": "^0.0.8",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.2.10"
  }
}
